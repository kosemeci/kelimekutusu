<body>
    <%- include('partials/navbar') %>
    <%- include('partials/main-routes') %>

        <main class="container" id="main-containers">
            <span class="container-title">
                Kutuya Tıkla Kelime Çalışmanı Başlat!            
            </span>
            <div class="mrntp"> 
                <a href="/seviye-ogrenme-testi" class="test-level-btn">Kelime Seviyeni Öğren</a>
                <a href="/kutum" class="test-level-btn">Kelime Kutunu Oluştur</a>
            </div>
            <div class="separator">
                <span>Seviyeler</span>
              </div>
            <div class="cards">
                <% words.forEach((word,index)=> { %>
                    <div class="card color<%=++index%> lazy-card hidden" id="<%= word.level %>" aria-label="<%= word.level %> seviyesi için kelime kutusu">
                        <div class="box level-a">
                            <% ['front', 'back', 'left', 'right', 'top', 'bottom'].forEach(face => { %>
                                <div class="face <%= face %>">
                                    <% if (face === 'front' || face === 'left' || face === 'bottom') { %>
                                        <%= word.level %>
                                    <% } %>
                                </div>
                            <% }) %>
                        </div>                       
                        <p>
                            <%= word.level %> seviyesinde <%= word.count %> kelime
                                    <p>
                    </div>
                    <% }) %>
            </div>
            <div class="separator">
                <span>Kategoriler</span>
              </div>
            <div class="cards">
                <% words_category.forEach((word,index)=> { %>
                    <div class="card color<%=++index+9%> lazy-card hidden" id="<%= word.category %>"aria-label="<%= word.category %> kategorisi için kelime kutusu">
                        <div class="box level-a">
                            <% ['front', 'back', 'left', 'right', 'top', 'bottom'].forEach(face => { %>
                                <div class="face <%= face %>">
                                    <% if (face === 'front' || face === 'left' || face === 'bottom') { %>
                                        <%= word.category %>
                                    <% } %>
                                </div>
                            <% }) %>
                        </div>
                        <p>
                            <% words_TrCategory.forEach((trword)=> { %>
                                <% if (word.category===trword.english) { %>
                                    <%= trword.turkish %> kategorisinde <br>
                                        <%= word.count %> kelime
                                            <% } %>
                                                <% }) %>
                        </p>
                    </div>
                    <% }) %>
            </div>
            <div>
                <a href="/kutum" class="test-level-btn">Kelime Kutunu Oluştur</a>
                <a href="/seviye-ogrenme-testi" class="test-level-btn">Kelime Seviyeni Öğren</a>
            </div>
            <%- include('partials/routes') %>
        </main>
        <script src="/js/index.js"></script>
</body>
